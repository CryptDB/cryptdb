SELECT no_o_id FROM new_order WHERE no_d_id = 2 AND no_w_id = 2 ORDER BY no_o_id ASC LIMIT 1 ;
DELETE FROM new_order WHERE no_d_id = 3  AND no_w_id = 2 AND no_o_id = 2;
SELECT o_c_id FROM oorder WHERE o_id = 1 AND o_d_id = 1 AND o_w_id = 2;
UPDATE oorder SET o_carrier_id = 3 WHERE o_id = 3 AND o_d_id = 3 AND o_w_id = 3;
UPDATE order_line SET ol_delivery_d = 'a' WHERE ol_o_id = 9 AND ol_d_id = 2 AND ol_w_id = 1;
SELECT SUM(ol_amount) AS ol_total FROM order_line WHERE ol_o_id = 2  AND ol_d_id = 2  AND ol_w_id = 2 ;
UPDATE customer SET c_balance = c_balance + 1, c_delivery_cnt = c_delivery_cnt + 1  WHERE c_id = 1 AND c_d_id = 1 AND c_w_id = 2;
SELECT o_id, o_carrier_id, o_entry_d FROM oorder WHERE o_w_id = 1 AND o_d_id = 1 AND o_c_id = 1 ORDER BY o_id DESC LIMIT 1;
